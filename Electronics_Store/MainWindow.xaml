<Window x:Class="Electronics_Store.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Electronics_Store"
        mc:Ignorable="d" Height="300" Width="293.863"
         WindowStartupLocation="CenterScreen"
        >
    <Grid>
        <StackPanel>
            <Image Source="logo.png" Width="75"></Image>
            <Label  Content="Вход" FontWeight="Bold" HorizontalAlignment="Center" FontSize="18" Margin="0 10 0 0" FontFamily="Comic Sans MS"></Label>
            <Label  Content="Логин" Margin="10 0 10 0" Style="{StaticResource labelSt}" ></Label>
            <TextBox  Margin="10 0 10 0" FontFamily="Comic Sans MS" FontSize="14" Name="loginTb"></TextBox>
            <Label  Content="Пароль" Margin="10 0 10 0" Style="{StaticResource labelSt}" ></Label>
            <TextBox Margin="10 0 10 0" FontFamily="Comic Sans MS" FontSize="14" Name="passwordTb"></TextBox>
            <Button Content="Войти" Margin="10 10 10 0" Height="30" FontSize="14" Click="Button_Click"></Button>
        </StackPanel>
    </Grid>
</Window>
